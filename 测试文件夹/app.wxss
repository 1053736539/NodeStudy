/* flex布局 */

/*上下左右都居中*/
.flex_c_c{ display: -webkit-box ; -webkit-box-align: center; -webkit-box-pack: center; display: flex; justify-content: center; align-items: center; }
/*横着的时候 上下居中 靠左对齐*/
.flex_c_l{ display: -webkit-box; -webkit-box-pack: start; -webkit-box-align: center; display: flex; justify-content: flex-start; align-items: center;}
/*横着的时候 上下居中 靠左对齐*/
.flex_c_r{ display: -webkit-box; -webkit-box-pack: end; -webkit-box-align: center; display: flex; justify-content: flex-end; align-items: center;}
/*横着的时候 靠上 靠左对齐*/
.flex_t_l{ display: -webkit-box; -webkit-box-pack: start; -webkit-box-align: start; display: flex; justify-content: flex-start; align-items: flex-start; }
/*横着的时候 靠下 靠左对齐*/
.flex_b_l{ display: -webkit-box; -webkit-box-pack: start; -webkit-box-align: end; display: flex; justify-content: flex-start; align-items: flex-end; }
/*横着的时候 靠下 靠右对齐*/
.flex_b_r{ display: -webkit-box; -webkit-box-pack: end; -webkit-box-align: end; display: flex; justify-content: flex-end; align-items: flex-end; }
/*横着的时候 靠上 左右居中对齐*/
.flex_t_c{ display: -webkit-box; -webkit-box-pack: center; -webkit-box-align: start; display: flex; justify-content: center; align-items: flex-start; }
/*横着的时候 上下居中 左右spacebetween*/
.flex_c_bet{ display: -webkit-box; -webkit-box-pack: justify; -webkit-box-align: center; display: flex; justify-content: space-between; align-items: center; }
/*横着的时候 靠上 左右spacebetween*/
.flex_t_bet{ display: -webkit-box; -webkit-box-pack: justify; -webkit-box-align: start; display: flex; justify-content: space-between; align-items: flex-start; }

/*横着的时候 靠上 靠右对齐*/
.flex_t_r{ display: -webkit-box; -webkit-box-pack: end; -webkit-box-align: start; display: flex; justify-content: flex-end; align-items: flex-start;}

/*竖着的时候 上下居中 左右对齐*/
.flex_c_c_col{ display: -webkit-box; -webkit-box-orient: vertical; -webkit-box-align: center;  -webkit-box-pack: center; display: flex; flex-direction: column;  align-items: center; justify-content: center; }
/*竖着的时候 上下居中 右对齐*/
.flex_c_r_col{ display: flex; flex-direction: column;  align-items: flex-end; justify-content: center; }

/*竖着的时候 上下居中 靠左对齐*/
.flex_c_l_col{ display: -webkit-box; -webkit-box-orient: vertical; -webkit-box-align: start; display: flex; flex-direction: column; justify-content: center;}
.flex_c_a{ display: flex; justify-content: space-around; }
.flex_bet_l_col{ display: flex;align-items: center; flex-direction: column; justify-content: space-between; }
.flex_b_c{ display: -webkit-box ; -webkit-box-align: center; -webkit-box-pack: center; display: flex; justify-content: center; align-items: baseline; }
.flex_c_c_col_s{ display: -webkit-box; -webkit-box-orient: vertical; -webkit-box-align: center;  -webkit-box-pack: center; display: flex; flex-direction: column;  align-items: center; justify-content: space-between;}




